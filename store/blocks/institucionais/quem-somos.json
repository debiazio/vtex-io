{
  "store.custom#quem-somos": {
    "blocks": ["flex-layout.row#quem-somos"]
  },
  "flex-layout.row#quem-somos": {
    "props": {
      "blockClass": "pagina-institucional"
    },
    "children": [
      "flex-layout.col#menu-institucionais-quem-somos",
      "flex-layout.col#conteudo-quem-somos"
    ],
    "title": "Página Envio de Produtos"
  },
  "flex-layout.col#menu-institucionais-quem-somos": {
    "props": {
      "width": "22%",
      "blockClass": "menu-institucionais"
    },
    "children": [
      "rich-text#titulo-menu-institucionais",
      "flex-layout.row#institucionais-quem-somos-active",
      "link#institucionais-politica-privacidade",
      "link#institucionais-formas-pgto",
      "link#institucionais-envio",
      "link#institucionais-trocas-e-devolucoes"
    ],
    "title": "Menu institucionais - Página Quem Somos"
  },

  "flex-layout.row#institucionais-quem-somos-active": {
    "props": {
      "blockClass": "menu-institucionais-active"
    },
    "children": [
      "flex-layout.col#titulo-quem-somos-active",
      "flex-layout.col#institucional-icon-active"
    ]
  },
  "flex-layout.col#titulo-quem-somos-active": {
    "props": {
      "blockClass": "col-titulo-inst"
    },
    "children": [
      "rich-text#titulo-item-menu-quem-somos-active"
    ]
  },
  "rich-text#titulo-item-menu-quem-somos-active": {
    "props": {
      "text": "[Quem Somos](/institucional/quem-somos)",
      "blockClass": "item-menu-institucionais-active"
    }
  },
  "flex-layout.col#institucional-icon-active": {
    "props": {
      "blockClass": "col-icon-inst"
    },
    "children": [
      "image#icon-menu-institucionais-active"
    ]
  },
  "image#icon-menu-institucionais-active": {
    "props": {
      "blockClass": "icone-institucionais-active",
      "src": "assets/seta-para-direita.svg",
      "alt": "Ícone seta para direita"
    }
  },

  "flex-layout.col#conteudo-quem-somos": {
    "children": [
      "rich-text#titulo-quem-somos",
      "flex-layout.col#imagem-quem-somos",
      "flex-layout.col#imagem-mobile-quem-somos"
    ],
    "props": {
      "width": "78%",
      "blockClass": "conteudo-institucionais"
    },
    "title": "Conteúdo Quem somos"
  },

  "rich-text#titulo-quem-somos": {
    "props": {
      "text": "## Quem Somos",
      "blockClass": "titulo-institucionais"
    },
    "title": "Titulo - Quem somos"
  },
  "flex-layout.col#imagem-quem-somos": {
    "props": {
      "blockClass": "container-imagem-quem-somos"
    },
    "children": [
      "image#quem-somos"
    ]
  },
  "image#quem-somos": {
    "props": {
      "blockClass": "imagem-quem-somos",
      "src": "assets/Esquema_timeline.webp",
      "alt": "Imagem timeline Idealine"
    }
  },
  "flex-layout.col#imagem-mobile-quem-somos":{
    "props": {
      "blockClass": "container-imagem-mobile-quem-somos"
    },
    "children": [
      "image#mobile1-quem-somos",
      "image#mobile2-quem-somos",
      "image#mobile3-quem-somos",
      "image#mobile4-quem-somos",
      "image#mobile5-quem-somos",
      "image#mobile6-quem-somos"
    ]
  },
  "image#mobile1-quem-somos":{
    "props": {
      "blockClass": "imagem-mobile-quem-somos",
      "src": "assets/01-.webp",
      "alt": "Imagem timeline Idealine"
    }
  },
  "image#mobile2-quem-somos":{
    "props": {
      "blockClass": "imagem-mobile-quem-somos",
      "src": "assets/02-.webp",
      "alt": "Imagem timeline Idealine"
    }
  },
  "image#mobile3-quem-somos":{
    "props": {
      "blockClass": "imagem-mobile-quem-somos",
      "src": "assets/03-.webp",
      "alt": "Imagem timeline Idealine"
    }
  },
  "image#mobile4-quem-somos":{
    "props": {
      "blockClass": "imagem-mobile-quem-somos",
      "src": "assets/04-.webp",
      "alt": "Imagem timeline Idealine"
    }
  },
  "image#mobile5-quem-somos":{
    "props": {
      "blockClass": "imagem-mobile-quem-somos",
      "src": "assets/05-.webp",
      "alt": "Imagem timeline Idealine"
    }
  },

  "image#mobile6-quem-somos":{
    "props": {
      "blockClass": "imagem-mobile-quem-somos",
      "src": "assets/06-.webp",
      "alt": "Imagem timeline Idealine"
    }
  }
}
