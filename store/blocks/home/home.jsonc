  "rich-text#shelf-title": {
    "props": {
      "text": "## **AUTOCLAVES DE 8 LITROS**",
      "blockClass": "shelfTitle"
    },
    "title": "Título prateleira"
  },
  "flex-layout.row#shelf": {
    "children": ["list-context.product-list#prateleira-8litros"]
  },
  "list-context.product-list#prateleira-8litros": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#prateleira-8litros"],
    "props": {
      "collection": "207"
    },
    "title": "Prateleira 8 litros"
  },
  "slider-layout#prateleira-8litros": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": true,
      "blockClass": "shelf",
      "showNavigationArrows": "desktopOnly"
    }
  },


  "flex-layout.row#shelf2": {
    "children": [
      "list-context.product-list#prateleira-maior-capacidade"
    ]
  },

  // linha da prateleira de maior capacidade
  "rich-text#prateleira-maior-capacidade": {
    "props": {
      "text": "## **AUTOCLAVES DE MAIOR CAPACIDADE**",
      "blockClass": "shelfTitle"
    },
    "title": "Título prateleira maior capacidade"
  },
  "list-context.product-list#prateleira-maior-capacidade": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#prateleira-maior-capacidade"],
    "props": {
      "orderBy": "OrderByBestDiscountDESC",
      "collection": "208"
    }
  },
  "slider-layout#prateleira-maior-capacidade": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": true,
      "blockClass": "shelf",
      "showNavigationArrows": "desktopOnly"
    }
  },

  "flex-layout.row#shelf3": {
    "children": [
      "list-context.product-list#prateleira-madu"
    ]
  },
  // linha da prateleira de mobiliario técnico
  "rich-text#prateleira-madu": {
    "props": {
      "text": "## **MOBILIÁRIO TÉCNICO MAD.U**",
      "blockClass": "shelfTitle"
    },
    "title": "Título prateleira MAD.U"
  },
  "list-context.product-list#prateleira-madu": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#prateleira-madu"],
    "props": {
      "orderBy": "OrderByBestDiscountDESC",
      "category": "19"
    }
  },
  "slider-layout#prateleira-madu": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": true,
      "blockClass": "shelf",
      "showNavigationArrows": "desktopOnly"
    }
  },


  "modal-trigger#aviso-cookies": {
    "props": {
      "trigger": "load"
    },
    "children": [
      "rich-text#texto-padrao",
      "modal-layout#aviso-cookies"
    ]
  },
  "rich-text#texto-padrao": {
    "props": {
      "text": "Click me",
      "blockClass": "texto-obrigatorio-modal"
    }
  },
  "modal-layout#aviso-cookies": {
    "props": {
      "blockClass": "modal-cookies",
      "scroll":"content",
      "disableEscapeKeyDown": false
    },
    "children": [
      "rich-text#modal-content",
      "modal-actions.close"
    ]
  },
  "rich-text#modal-content": {
    "props": {
      "text": "UTILIZAMOS COOKIES PARA MELHORAR NOSSO SITE E SUA EXPERIÊNCIA DE COMPRA. AO CONTINUAR NAVEGANDO EM NOSSO SITE, **VOCÊ ESTÁ DE ACORDO CONFORME NOSSA POLÍTICA DE PRIVACIDADE E TERMOS DE USO, E NOSSA POLÍTICA QUANTO À UTILIZAÇÃO DE COOKIES.**",
      "blockClass": "texto-cookies-aviso"
    }
  },
  "modal-actions.close": {
    "props": {
      "label": "Aceitar e continuar",
      "blockClass": "botao-modal-cookies"
    }
  }


}
