  // BRINDE BLACK FRIDAY HORIZONTAIS + BF
"condition-layout.product#brinde-necessaire-horizontais-bf":{
  "props": {
    "conditions": [{
      "subject": "specificationProperties",
      "arguments": {
        "name": "Posicao",
        "value": "Horizontal"
      }
    }],
    "Then": "flex-layout.row#brinde-horizontais-blackFriday"
  }
},
"flex-layout.row#brinde-horizontais-blackFriday":{
  "props": {
    "blockClass": "condicional-brinde-5l-8l"
  },
  "children": [
    "image#brinde-horizontais-blackFriday"
  ]
},
"image#brinde-horizontais-blackFriday":{
  "props": {
    "src": "assets/necessaire.png",
    "alt": "Imagem com brinde da Black Friday - horizontais",
    "blockClass": "img-brindetriplo-bf-5l-8l"
  }
},

  // BRINDE BLACK FRIDAY 5L + BF
  "condition-layout.product#brinde-autoclaves-5l": {
    "props": {

      "conditions": [{
          "subject": "specificationProperties",
          "arguments": {
            "name": "Capacidade",
            "value": "5L"
          }
        }
        ,{
          "subject": "categoryId",
          "arguments": {
            "id": "69"
          }
        }
      ],
      "matchType": "all",
      "Then": "flex-layout.row#custom-pdp-brinde-5l-8l-black-friday"
    }
  },
  "condition-layout.product#brinde-autoclaves-5litros": {
    "props": {

      "conditions": [{
          "subject": "specificationProperties",
          "arguments": {
            "name": "Capacidade",
            "value": "5 Litros"
          }
        }
        ,{
          "subject": "categoryId",
          "arguments": {
            "id": "69"
          }
        }
      ],
      "matchType": "all",
      "Then": "flex-layout.row#custom-pdp-brinde-5l-8l-black-friday"
    }
  },
    // BRINDE BLACK FRIDAY 8L + BF
