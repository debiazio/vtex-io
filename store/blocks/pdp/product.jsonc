{
  "store.product": {
    "children": [
      "flex-layout.row#script_rd",
      "responsive-layout.desktop#gif-animado-parcelamento",
      "responsive-layout.mobile#gif-animado-parcelamento",

      // "rich-text#titulo-pagina",
      "flex-layout.row#product-breadcrumb",
      "responsive-layout.desktop#availability",
      "responsive-layout.mobile#availability",

      // "product-gifts",
      "rich-text#certificacoes-titulo",
      "flex-layout.row#certificacoes"
    ]
  },
  // LAYOUT DO DESKTOP LINHA DO GIF
  "responsive-layout.desktop#gif-animado-parcelamento": {
    "children": [
      "flex-layout.row#banner-superior-desktop"
    ]
  },
  //linha do banner(gif) DESKTOP
  "flex-layout.row#banner-superior-desktop": {
    "props": {
      "blockClass": "gif-animado"
    },
    "children": [
      "image#banner-gif"
    ]
  },
  "image#banner-gif": {
    "props": {
      "blockClass": "gif-animado-parcelas-img",
      "src": "assets/banner_10x.gif",
      "alt": "Banner Image",
      "width": 1120,
      "height": 80
    }
  },
  // LAYOUT DO MOBILE LINHA DO GIF
  "responsive-layout.mobile#gif-animado-parcelamento": {
    "children": [
      "flex-layout.row#banner-superior-mobile"
    ]
  },
  //linha do banner(gif) MOBILE
  "flex-layout.row#banner-superior-mobile": {
    "props": {
      "blockClass": "gif-animado"
    },
    "children": [
      "image#mobile-banner-gif"
    ]
  },
  "image#mobile-banner-gif": {
    "props": {
      "blockClass": "gif-animado-parcelas-img",
      "src": "assets/banner_10x.gif",
      "alt": "Banner Image",
      "width": 1600,
      "height": 40
    }
  },


  // linha que mostra o caminho até o conteúdo
  "flex-layout.row#product-breadcrumb": {
    "props": {
      "blockClass": "breadcrumb"
    },
    "children": ["breadcrumb"]
  },
  // LAYOUT DESKTOP
  "responsive-layout.desktop#availability": {
    "children": [
      "condition-layout.product#availability"
    ]
  },
  // LAYOUT MOBILE
  "responsive-layout.mobile#availability": {
    "children": [
      "condition-layout.product#mobile-availability"
    ]
  },
  // avalia se o produto tem estoque
  "condition-layout.product#mobile-availability": {
    "props": {
      "conditions": [{
        "subject": "isProductAvailable"
      }],
      "Then": "flex-layout.row#mobile-product-main",
      "Else": "flex-layout.row#product-availability"
    }
  },
  // avalia se o produto tem estoque desktop
  "condition-layout.product#availability": {
    "props": {
      "conditions": [{
        "subject": "isProductAvailable"
      }],
      "Then": "flex-layout.row#product-main",
      "Else": "flex-layout.row#product-availability"
    }
  },
  // linha do produto
  "flex-layout.row#product-main": {
    "props": {
      "blockClass": "product-main"
    },
    "children": [
      "flex-layout.col#product-left-col",
      "flex-layout.col#product-right-col"
    ]
  },
  // linha do produto
  "flex-layout.row#mobile-product-main": {
    "props": {
      "blockClass": "product-main"
    },
    "children": [
      "flex-layout.col#mobile-product-left-col"
      // "flex-layout.col#product-right-col"
    ]
  },
  // coluna da esquerda MOBILE
  "flex-layout.col#mobile-product-left-col": {
    "children": [
      "flex-layout.row#product-name",
      "flex-layout.row#add-to-list-btn",
      "stack-layout",
      "product-rating-summary", // avaliações
      "flex-layout.row#list-price-savings", // preco sem desconto, riscado
      "flex-layout.row#selling-price", // preco a vista
      "flex-layout.row#linha-parcelas",

      "sku-selector",
      "flex-layout.row#mobile-quantidade-cep",
      "flex-layout.row#vantagens-pgto-a-vista",
      // "product-gifts",
      // "product-questions-and-answers",
      // "flex-layout.row#video",
      "condition-layout.product#branco-perola",
      "condition-layout.product#rosa",
      "condition-layout.product#rosa-padrao2",

      "condition-layout.product#rosa-gold",
      "condition-layout.product#rosa-gold-padrao2",

      "condition-layout.product#rose-gold-laBelle-8l",
      "condition-layout.product#rose-gold-labelle-8l-padrao2",
      "condition-layout.product#rose-gold-idealclave-8l",
      "condition-layout.product#rose-gold-idealclave-8l-padrao2",
      "condition-layout.product#perola-idealclave-8l",
      "condition-layout.product#perola-idealclave-8l-padrao2",
      "condition-layout.product#perola-8l-labelle",
      "condition-layout.product#perola-8l-labelle-padrao2",
      "condition-layout.product#rosa-idealclave-8l",
      "condition-layout.product#rosa-idealclave-8l-padrao2",
      "condition-layout.product#rosa-laBelle-8l",
      "condition-layout.product#rosa-laBelle-8l-padrao2",
      "flex-layout.row#description",
      // "flex-layout.row#specifications-title",
      "product-specification-group#table",
      "flex-layout.row#relatedProducts",
      "flex-layout.row#reviews-and-ratings"

    ],
    "props": {
      "width": "60%"
    }
  },

  // LINHA DE PREÇO PARCELADO MOBILE
  "flex-layout.row#linha-parcelas": {
    "props": {
      "blockClass": "parcelas-mobile"
    },
